<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
        <record id="view_account_analytic_line_form_state_inherit_account" model="ir.ui.view">
            <field name="name">account.analytic.line.form.state.inherit.account</field>
            <field name="model">account.analytic.line</field>
            <field name="inherit_id" ref="analytic.view_account_analytic_line_form"/>
            <field name="arch" type="xml">
                <data>
                    <xpath expr="//field[@name='date']" position="after">
                        <field name="state"/>
                    </xpath>
                </data>
            </field>
        </record>
        <record id="view_account_analytic_line_tree_state_inherit_account" model="ir.ui.view">
            <field name="name">account.analytic.line.tree.state.inherit.account</field>
            <field name="model">account.analytic.line</field>
            <field name="inherit_id" ref="analytic.view_account_analytic_line_tree"/>
            <field name="arch" type="xml">
                <data>
                    <xpath expr="//field[@name='tag_ids']" position="after">
                        <field name="state"/>
                    </xpath>
                </data>
            </field>
        </record>
    </data>
</openerp>
